<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeRectangleFilletColorSolidBottomRightPressed"/>
            <corners android:bottomLeftRadius="0dp"
                     android:bottomRightRadius="@dimen/shapeRectangleFilletRadiusBottomRight"
                     android:topLeftRadius="0dp"
                     android:topRightRadius="0dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeRectangleFilletColorSolidBottomRightDefault"/>
            <corners android:bottomLeftRadius="0dp"
                     android:bottomRightRadius="@dimen/shapeRectangleFilletRadiusBottomRight"
                     android:topLeftRadius="0dp"
                     android:topRightRadius="0dp"/>
        </shape>
    </item>
</selector>