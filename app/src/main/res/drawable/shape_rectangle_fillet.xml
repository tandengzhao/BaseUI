<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeBorderColorSolidPressed"/>
            <corners android:radius="@dimen/shapeRectangleFilletRadius"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeBorderColorSolidDefault"/>
            <corners android:radius="@dimen/shapeRectangleFilletRadius"/>
        </shape>
    </item>
</selector>