<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeRectangleFilletColorSolidTopLeftRightPressed"/>
            <corners android:bottomLeftRadius="0dp"
                     android:bottomRightRadius="0dp"
                     android:topLeftRadius="@dimen/shapeRectangleFilletRadiusTopLeft"
                     android:topRightRadius="@dimen/shapeRectangleFilletRadiusTopRight"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/shapeRectangleFilletColorSolidTopLeftRightDefault"/>
            <corners android:bottomLeftRadius="0dp"
                     android:bottomRightRadius="0dp"
                     android:topLeftRadius="@dimen/shapeRectangleFilletRadiusTopLeft"
                     android:topRightRadius="@dimen/shapeRectangleFilletRadiusTopRight"/>
        </shape>
    </item>
</selector>